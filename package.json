  "description": "Model Context Protocol server for Google AI services (VEO 3, Imagen 4, Gemini, Lyria 2)",
  "main": "index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "google-ai-mcp": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "lint": "eslint src --ext ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "npm run build",
    "verify": "node verify-deployment.js"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "google",
    "vertex-ai",
    "veo",
    "imagen",
    "gemini",
    "lyria",
    "ai",
    "video-generation",
    "image-generation",
    "llm"
  ],  "author": {
    "name": "Steve Kaplan",
    "email": "steve@stevekaplan.ai",
    "url": "https://stevekaplan.ai"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Stevekaplanai/google-ai-mcp-server.git"
  },
  "bugs": {
    "url": "https://github.com/Stevekaplanai/google-ai-mcp-server/issues"
  },
  "homepage": "https://github.com/Stevekaplanai/google-ai-mcp-server#readme",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.5.0",
    "axios": "^1.6.8",
    "dotenv": "^16.4.5",
    "google-auth-library": "^9.6.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.11",
    "prettier": "^3.2.4",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
